<launch>
  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find farmbot_gazebo)/worlds/farmbot.world"/>
    <!-- more default parameters can be changed here -->
  </include>

  <node pkg="gazebo_ros" type="spawn_model" name="spawn_model" args="-file $(find farmbot_description)/urdf/parts/raised_bed.urdf -urdf -x 0 -y 0 -z 1 -model farmbot" >
  </node> 
</launch>